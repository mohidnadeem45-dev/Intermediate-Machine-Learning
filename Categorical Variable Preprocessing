    "from sklearn.preprocessing import OneHotEncoder\n",
    "\n",
    "# Use as many lines of code as you need!\n",
    "\n",
    "OHE =  OneHotEncoder(handle_unknown='ignore', sparse=False)\n",
    "n_xtrain = X_train.drop(high_cardinality_cols, axis=1)\n",
    "n_xvalid = X_valid.drop(high_cardinality_cols, axis=1)\n",
    "\n",
    "Oh_cols_train = pd.DataFrame(OHE.fit_transform(n_xtrain[low_cardinality_cols]))\n",
    "Oh_cols_valid = pd.DataFrame(OHE.transform(n_xvalid[low_cardinality_cols]))\n",
    "\n",
    "Oh_cols_train.index = n_xtrain.index\n",
    "Oh_cols_valid.index = n_xvalid.index\n",
    "\n",
    "num_x_train = X_train.drop(object_cols,axis=1)\n",
    "num_x_valid = X_valid.drop(object_cols,axis=1)\n",
    "\n",
    "OH_X_train = pd.concat([num_x_train,Oh_cols_train],axis=1)\n",
    "OH_X_valid = pd.concat([num_x_valid,Oh_cols_valid],axis=1)\n",
    "\n",
    "OH_X_train.columns = OH_X_train.columns.astype(str)\n",
    "OH_X_valid.columns = OH_X_valid.columns.astype(str)\n",
    "# Check your answer\n",
    "step_4.check()"
   ]
